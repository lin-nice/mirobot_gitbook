{
    "title": "Mirobot使用教程",
    "description": "插件：评论、搜索、github、编辑",
    "author": "curvaboy",
    "language": "zh-hans",
    "root": ".",

    "plugins": [
        "github",
        "disqus",
        "-search",
        "search-pro",
        "tbfed-pagefooter"
    ],

    "pluginsConfig": {
        "github": {
            "url": "https://github.com/lin-nice"
        },
        "disqus": {
        "shortName": "gitbookuse"
        },
        "search-pro": {
        "cutWordLib": "nodejieba",
        "defineWord" : ["Gitbook Use"]
        },
        "tbfed-pagefooter": {
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
        }

    }
}
