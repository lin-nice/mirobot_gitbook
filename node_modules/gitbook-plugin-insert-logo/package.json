{
  "description": "GitBook Plugin to insert (url or file or base64) into the navigation above search input.",
  "name": "gitbook-plugin-insert-logo",
  "version": "0.1.5",
  "keywords": [
    "gitbook",
    "plugin",
    "insert-logo",
    "logo",
    "github"
  ],
  "main": "index.js",
  "author": {
    "name": "matusnovak",
    "email": "matusnov@gmail.com"
  },
  "readme": "GitBook Plugin to insert logo (url or file or base64) into the navigation above search input.",
  "engines": {
    "gitbook": ">1.x.x"
  },
  "gitbook": {
    "properties": {
      "url": {
        "type": "string",
        "default": "http://www.example.com/my-logo.png",
        "title": "Logo URL"
      },
      "style": {
        "type": "string",
        "default": "background: none;",
        "title": "Custom CSS"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matusnovak/gitbook-plugin-insert-logo.git"
  }
}
